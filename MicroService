 tables:
CREATE TABLE BRS.USER (
  id INTEGER NOT NULL  PRIMARY KEY,
  username VARCHAR(250) NOT NULL,
  password VARCHAR(250) NOT NULL,
  email VARCHAR(250) DEFAULT NULL,
  active smallint  DEFAULT 0,
  isAdmin smallint  DEFAULT 0 
);


create table BRS.bus_ticket(
  ticket_id integer NOT NULL PRIMARY KEY,
  bus_id integer NOT NULL,
  seat_id integer NOT NULL
)

create table BRS.bus_book(
  bus_id integer NOT NULL PRIMARY KEY,
  bus_source varchar(20) NOT NULL,
  bus_destination varchar(20) NOT NULL,
  bus_source_time timestamp NOT NULL,
  bus_destination_time timestamp NOT NULL
)


create table BRS.bus_category(
  bus_id integer NOT NULL PRIMARY KEY,
  bus_category varchar(10) DEFAULT 'NORMAL'
)


create table BRS.BUS_ROUTE(
  bus_route integer NOT NULL PRIMARY KEY,
  origin varchar(20) NOT NULL,
  destination varchar(20) NOT NULL
);


create table BRS.Payment(
  booking_id integer NOT NULL PRIMARY KEY,
  paymnent_mode varchar(20) NOT NULL,
  paymnent_status varchar(20) NOT NULL
)


