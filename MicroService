create table customer(
  customer_id integer NOT NULL PRIMARY KEY,
  ticket_id integer NOT NULL,
  is_Admin SMALLINT NOT NULL WITH DEFAULT 0,
  FOREIGN KEY bus_ticlet_fkey (ticket_id) 
  REFERENCES bus_ticket ON DELETE NO ACTION)


CREATE TABLE BRS.USER (
  user_id INTEGER NOT NULL  PRIMARY KEY,
  username VARCHAR(25) NOT NULL,
  password VARCHAR(25) NOT NULL,
  email VARCHAR(25) DEFAULT NULL,
  status  smallint  DEFAULT 0,
  isAdmin smallint  DEFAULT 0,
  ticket_id VARCHAR(25) default 0
);




  
  DB schema:
  create table bus_book(
  bus_id integer NOT NULL PRIMARY KEY,
  bus_source varchar(20) NOT NULL,
  bus_destination varchar(20) NOT NULL,
  bus_source_time timestamp NOT NULL,
  bus_destination_time timestamp NOT NULL
)


insert into bus_book values(1,'solan','salogra',(TIMESTAMP(CAST('04.02.2005' AS VARCHAR(10)),'13:14:53')),(TIMESTAMP(CAST('05.02.2005' AS VARCHAR(10)),'13:14:53')));

select * from bus_book;

create table bus_category(
  bus_id integer NOT NULL PRIMARY KEY,
  bus_category varchar(10) DEFAULT 'NORMAL'
)


insert into bus_category values(1111,'DELUX');

select * from bus_category;

select * from bus_book a where a.bus_id=bus_category.bus_id;

SELECT 
   b.bus_id,
   p.bus_id,
   b.bus_source_time
FROM 
    bus_book b
INNER JOIN bus_category p 
    ON p.bus_id = b.bus_id;


create table bus_ticket(
  ticket_id integer NOT NULL PRIMARY KEY,
  bus_id integer NOT NULL,
  seat_id integer NOT NULL
)

/* Foreign key contraint as delete cascade */

/* 1 is for Admin */
create table customer(
  customer_id integer NOT NULL PRIMARY KEY,
  ticket_id integer NOT NULL,
  is_Admin SMALLINT NOT NULL WITH DEFAULT 0,
  FOREIGN KEY bus_ticlet_fkey (ticket_id) 
  REFERENCES bus_ticket ON DELETE NO ACTION)
  
  
   insert into bus_ticket values(2,3,4);
   insert into bus_ticket values(3,3,4);
   
   insert into customer values(1,2,0);
   
   insert into customer values(4,3,0);
    
  
  drop table customer;
  
  
  select * from bus_ticket;
  
create table customer_profile(
  cust_id integer NOT NULL PRIMARY KEY,
  address varchar(20) NOT NULL,
  email varchar(20) NOT NULL
)  
  
